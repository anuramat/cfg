# `swaymsg -t get_outputs` or `wlr-randr`

set {
    $internal eDP-1
    $external DP-1 DP-2
}

workspace {
    1 output $internal
    2 output $internal
    3 output $internal
    4 output $internal
    5 output $internal
    6 output $external $internal
    7 output $external $internal
    8 output $external $internal
    9 output $external $internal
    10 output $external $internal
}
